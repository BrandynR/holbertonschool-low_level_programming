#include <stdio.h>
#include <stdarg.h>

/**
 *sum_them_all - returns the sum of all its parameters
 *
 *Return: sum or if n==0 return 0
 */

int sum_them_all(const unsigned int n, ...)
{
	va_list valist;
double sum = 0.0;
int i;
va_start(valist, n);

for (i = 0; i < n; i++)
{
	sum += va_arg(valist, int0;
		      }
		va_end(valist);
	return (sum);
}
}
